extends layout

block content
  h1= title
  - let image_names = ["1.jpg", "2.jpg", "3.jpg", "4.jpg", "5.jpg"]
  - let uniqueImages = []

  - while(uniqueImages.length < 3) {
    - let ran = Math.floor(Math.random() * 5)
    - if(!uniqueImages.includes(ran)) {
      - uniqueImages.push(ran)
      - let imgName = image_names[ran]
      img(src=`/images/${imgName}` alt=`Image No ${ran}` height="300px" width="300px")
    - }
  - }
